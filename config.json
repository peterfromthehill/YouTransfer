{
	"PORT": 5000,

	"general": {
		"title": "YouTransfer",
		"subtitle": "The self-hosted file sharing solution",
		"baseUrl": "http://localhost:5000",
		"cleanupSchedule": "*/1 * * * *",
		"schedulerEnabled": false,
		"enableDownload": true
	},

	"dropzone": {
		"maxFilesize": "2000",
		"parallelUploads": 20,
		"forceFallback": false
	},

	"storage": {
		"location": "local",
		"localstoragepath": "./uploads",
		"retention": 1,
		"retentionUnit": "weeks"
	},

	"state": {}

}
